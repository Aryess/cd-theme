{% block head %}
{% endblock %}
</head>
{% endblock %}

<body class="{{body_class}} container" data-template="base.twig">
        {% include 'header.twig' %}
	<section id="content" role="main" class="content-wrapper">
		{% if title %}<h1>{{title}}</h1>{% endif %}
		<div class="wrapper {{sidebar_class}}">
			{% block content %}
			Sorry, no content
			{% endblock %}
		</div>
		{% if sidebar %}
		<aside class="layout-sidebar">
			{{sidebar}}
		</aside>
		{% endif %}
	</section>

      {% include 'cta.twig' with {'buttontext': 'Call to Action', 'target': 'http://www.google.com' } %}
    <p/>

	{% block footer %}
	<footer id="find-dojo">
		{% include 'find-dojo.twig' %}
	</footer>
	<footer id="footer">
		{% include 'footer.twig' %}
	</footer>
	{{ function('wp_footer') }}
	<script type = "text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"/>
	{% endblock %}
</body>
</html>
