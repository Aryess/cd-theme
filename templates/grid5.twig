{# grid5.twig #}
{% block content %}
  <div class="wrapper-grid">
    <div class="grid">
      <h3>{{gridtitle}}</h3>
      <div class="content-wrapper">
        <div class="row">
        {% for item in grid %}
          <div class="col-xs-6 col-sm-4 col-md-2 grid-item imgWrap">
            <img src="{{item.src}}" alt="{{item.body}}" border="0">
            <span>{{item.description}}</span>
          </div>
        {% endfor %}
        </div>
      </div>
    </div>
  </div>
{% endblock %}
