{# special-header.twig #}

{% block content %}
<div class="special-header">
      <div class="row">
        <div class="col-md-2">
          <div class="image-container">
            <div class="image-crop"><img src="{{special.logo}}" border="0"></div>
          </div>
        </div>
        <div class="col-md-8">
          <div id="title">{{special.title}}</div> 
          <div id="link">{{special.link}}</div>
        </div>
      </div>
</div>
<div class="special-header-body">
  <div class="row">
    <div class="col-md-2"> </div>
    <div class="col-md-8">
      {{special.desc}}
    </div>
  </div>
</div>
{% endblock %}
