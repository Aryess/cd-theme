{% extends "base.twig" %}

{% block content %}
<div class="content-wrapper">
  <img src="{{site.theme.link}}/static/img/headers/header.png" class="top-banner" />
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    <section class="article-content">
      <div class="article-wrapper">
        <h1 class="article-h1 center">{{post.title}}</h1>
        <div class="article-body">
          {{broken_content[0]}}
        </div>
      </div>
      <div class="about-blocks">
        <div class="row top">
          {% include "item-box.twig" with {'title': '', 'hrefOne':'movement', 'hrefTwo':'foundation', 'rows': 2 , 'imageOne': site.theme.link ~ '/static/img/headers/movement.png', 'imageTwo': site.theme.link ~ '/static/img/headers/foundation.png'} %}
          <div class="col-md-6  double-par" >
            {{broken_content[1]}}
          </div>
          <div class="col-md-6 double-par-2">
            {{broken_content[2]}}
          </div>
        </div>
        <div class="row bottom">
         {% include "item-box.twig" with {'title': '', 'hrefThree':'supporters', 'hrefFour':'community-committee', 'rows': 2, 'imageOne': site.theme.link ~ '/static/img/headers/supporters.png', 'imageTwo': site.theme.link ~ '/static/img/headers/community-committee.png'} %}
          <div class="col-md-6 double-par">
            {{broken_content[3]}}
          </div>
          <div class="col-md-6 double-par-2">
            {{broken_content[4]}}
          </div>
        </div>
      </div>
    </section>
  </article>
</div>
{% endblock %}
