{% extends "base.twig" %}

{% block content %}
<div class="content-wrapper">
  <img src="/cd-wordpress/wp-content/uploads/2016/06/header.png" class="top-banner" />
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    <section class="article-content">
      <div class="article-wrapper">
        <h1 class="article-h1 center">{{post.title}}</h1>
        <div class="article-body">
          {{broken_content[0]}}
        </div>
      </div>
      <div class="about-blocks">
        <div class="row top">
          {% include "item-box.twig" with {'title': '', 'hrefOne':'movement', 'hrefTwo':'foundation', 'rows': 2 , 'imageOne': '/cd-wordpress/wp-content/themes/cd-theme/static/img/movement.png', 'imageTwo': '/cd-wordpress/wp-content/themes/cd-theme/static/img/foundation.png'} %}
          <div class="col-md-6">
            <p class="pull-right">{{broken_content[1]}}</p>
          </div>
          <div class="col-md-6">
            <p class="pull-left">{{broken_content[2]}}</p>
          </div>
        </div>
        <div class="row bottom">
         {% include "item-box.twig" with {'title': '', 'hrefThree':'supporters', 'hrefFour':'community-committee', 'rows': 2, 'imageOne': '/cd-wordpress/wp-content/themes/cd-theme/static/img/supporters.png', 'imageTwo': '/cd-wordpress/wp-content/themes/cd-theme/static/img/community-committee.png'} %}
          <div class="col-md-6">
            <p class="pull-right">{{broken_content[3]}}</p>
          </div>
          <div class="col-md-6">
            <p class="pull-left">{{broken_content[4]}}</p>
          </div>
        </div>
      </div>
    </section>
  </article>
</div>
{% endblock %}
