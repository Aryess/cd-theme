{% extends "base.twig" %}

{% block content %}
<div class="content-wrapper">
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    <section class="article-content">
      <div class="article-wrapper">
        <h1 class="article-h1 center">{{post.title}}</h1>
        <div class="article-body">
          {{broken_content[0]}}
        </div>
      </div>
      {% include "image-row-text.twig" with {'rows': 2 , 'paragraphOne': 'Some text.', 'paragraphTwo': 'Another paragraph',
      'paragraphThree': 'Another paragraph', 'paragraphFour': 'Another paragraph', 'titleOne': 'Movement'} %}

    </section>
  </article>
</div>
{% endblock %}
